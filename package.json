{
  "name": "ehsm_portal",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^4.0.33",
    "@sap/ux-ui5-tooling": "1",
    "@sap-ux/ui5-middleware-fe-mockserver": "2"
  },
  "scripts": {
    "start": "npx fiori run --open \"test/flp.html#app-preview\"",
    "start-local": "npx fiori run --config ./ui5-local.yaml --open \"test/flp.html#app-preview\"",
    "build": "npx ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "npx fiori verify",
    "deploy-config": "npx fiori add deploy-config",
    "start-noflp": "npx fiori run --open \"/index.html?sap-ui-xx-viewCache=false\"",
    "start-mock": "npx fiori run --config ./ui5-mock.yaml --open \"test/flp.html#app-preview\"",
    "int-test": "npx fiori run --config ./ui5-mock.yaml --open \"test/integration/opaTests.qunit.html\"",
    "start-variants-management": "npx fiori run --open \"/preview.html#app-preview\"",
    "unit-test": "npx fiori run --config ./ui5-mock.yaml --open \"test/unit/unitTests.qunit.html\""
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
